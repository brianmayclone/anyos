// -*- C++ -*-
// iomanip â€” I/O manipulators for anyOS libcxx
#ifndef _ANYOS_IOMANIP
#define _ANYOS_IOMANIP

#include <iostream>

namespace std {

struct _Setfill { char _fill; };
struct _Setprecision { int _prec; };

inline _Setfill setfill(char c) { return {c}; }
inline _Setprecision setprecision(int n) { return {n}; }

inline ostream& operator<<(ostream& os, const _Setfill&) { return os; }
inline ostream& operator<<(ostream& os, const _Setprecision&) { return os; }

using ios = ios_base;

} // namespace std

#endif // _ANYOS_IOMANIP
