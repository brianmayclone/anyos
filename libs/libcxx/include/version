#ifndef _LIBCXX_VERSION
#define _LIBCXX_VERSION

/* C++17 features */
#define __cpp_lib_optional          201606L
#define __cpp_lib_variant           201606L
#define __cpp_lib_any               201606L
#define __cpp_lib_string_view       201606L
